<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Level_Change_After_Start</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISCHANGED(Level__c),
    NOT(ISBLANK(Course_Start_Date__c)),
    TODAY() &gt;= Course_Start_Date__c
)</errorConditionFormula>
    <errorMessage>You cannot change the Level after the course has started.</errorMessage>
</ValidationRule>
